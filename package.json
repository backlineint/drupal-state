{
  "name": "@gdwc/drupal-state",
  "version": "1.0.6",
  "files": [
    "dist",
    "src/drupalState.{js,ts}",
    "src/fetch/*.{js,ts}",
    "src/types/*.ts"
  ],
  "main": "./dist/drupal-state.umd.js",
  "module": "./dist/drupal-state.es.js",
  "exports": {
    ".": {
      "import": "./dist/drupal-state.es.js",
      "require": "./dist/drupal-state.umd.js"
    },
    "./fetchApiIndex": "./src/fetch/fetchApiIndex",
    "./fetchJsonapiEndpoint": "./src/fetch/fetchJsonapiEndpoint",
    "./types": "./src/types",
    "./": "./"
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid",
    "proseWrap": "always"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{md,mdx}": "prettier --write"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "typedoc": "typedoc",
    "test": "jest --coverage",
    "eslint": "eslint --ext .js,.ts src --ignore-path .gitignore",
    "eslint:fix": "eslint --ext .js,.ts src --fix --ignore-path .gitignore",
    "prettier": "prettier \"**/*.{js,ts,md}\" --check --ignore-path .gitignore",
    "prettier:fix": "prettier \"**/*.{js,ts,md}\" --write --ignore-path .gitignore",
    "lint-staged": "lint-staged",
    "prepare": "npm run build && husky install",
    "postpublish": "rm src/*.js && rm src/**/*.js",
    "postinstall": "patch-package",
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "https://git.drupalcode.org/project/drupal_state.git"
  },
  "homepage": "https://www.drupal.org/project/drupal_state",
  "bugs": {
    "url": "https://www.drupal.org/project/issues/drupal_state"
  },
  "keywords": [
    "drupal",
    "decoupled",
    "zustand"
  ],
  "license": "GPL-2.0-or-later",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "fetch-mock-jest": "^1.5.1",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.2",
    "prettier": "2.3.2",
    "release-it": "^14.11.5",
    "typedoc": "^0.21.6",
    "typescript": "^4.3.2",
    "vite": "^2.4.4"
  },
  "dependencies": {
    "@types/isomorphic-fetch": "0.0.35",
    "@types/jest": "^27.0.1",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^27.0.6",
    "jest-junit": "^12.2.0",
    "jsona": "^1.9.2",
    "patch-package": "^6.4.7",
    "ts-jest": "^27.0.5",
    "zustand": "^3.5.7"
  }
}
