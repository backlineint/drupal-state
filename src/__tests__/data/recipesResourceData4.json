      "field_ingredients": [
        "225g butter or margarine",
        "225g caster sugar",
        "225g self-raising flour",
        "4 eggs",
        "1 tsp baking powder",
        "3 tbsp of jam for the filling",
        "Icing sugar to dust the top",
        "Cream to serve"
      ],
      "field_number_of_servings": 10,
      "field_preparation_time": 20,
      "field_recipe_instruction": {
        "value": "<ol>\n  <li>Preheat the oven to 350째F/180째C and grease two 8 inch cake tins.</li>\n  <li>In a large bowl, mix the butter and sugar together, then add the eggs, flour and baking powder.</li>\n  <li>Spread the mix evenly between the 2 cake tins.</li>\n  <li>Place both tins in the middle of the preheated oven for 20 minutes, before checking with a knife. When the knife comes out clean, it's ready!</li>\n  <li>Allow to cool in the tins before moving them onto a cooling rack.</li>\n  <li>Add the jam, stack, dust with icing sugar and serve with a big dollop of cream. Enjoy!</li>\n</ol>\n",
        "format": "basic_html",
        "processed": "<ol><li>Preheat the oven to 350째F/180째C and grease two 8 inch cake tins.</li>\n<li>In a large bowl, mix the butter and sugar together, then add the eggs, flour and baking powder.</li>\n<li>Spread the mix evenly between the 2 cake tins.</li>\n<li>Place both tins in the middle of the preheated oven for 20 minutes, before checking with a knife. When the knife comes out clean, it's ready!</li>\n<li>Allow to cool in the tins before moving them onto a cooling rack.</li>\n<li>Add the jam, stack, dust with icing sugar and serve with a big dollop of cream. Enjoy!</li>\n</ol>"
      },
      "field_summary": {
        "value": "A traditional Victoria sponge cake, perfect for any afternoon with a cup of tea.",
        "format": "basic_html",
        "processed": "<p>A traditional Victoria sponge cake, perfect for any afternoon with a cup of tea.</p>\n"
      }
